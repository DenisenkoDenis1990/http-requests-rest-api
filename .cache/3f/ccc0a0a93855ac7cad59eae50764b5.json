nhtml.toAttributes = function toAttributes(hash) {\n  var res = '';\n  var keys = Object.keys(hash);\n  for (var i = 0; i < keys.length; i++) {\n    var val = hash[keys[i]];\n    if (val === true) {\n      res += ' ' + keys[i];\n    } else {\n      res += ' ' + keys[i] + '=\"' + String(val) + '\"';\n    }\n  }\n  return res;\n};\n"},"lineCount":null}},"error":null,"hash":"d5f12c4a406a6c66b34724d1c7e0c07e","cacheData":{"env":{}}}